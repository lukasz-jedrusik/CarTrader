{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "keycloak": {
    "AuthorityUrl": "{keycloak-url}",
    "ClientId": "{client-id}"
  },
  "ConnectionStrings": {
    "BloggingDatabase": "Server={your_server};Database={your_db};User Id=dev;password={your_password};Trusted_Connection=False;MultipleActiveResultSets=true;"
  }
}
